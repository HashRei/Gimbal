{
  "name": "Savor-Finance",
  "version": "1.0",
  "scripts": {
    "hardhat": "hardhat",
    "compile": "hardhat compile",
    "hardhat:rinkeby": "hardhat --network rinkeby",
    "hardhat:mumbai" : "npx hardhat --network mumbai",
    "bridgerton:rinkeby:deploy": "npm run compile && npm run hardhat:rinkeby -- deploy-bridgerton --verify",
    "bridgerton:mumbai:deploy": "npm run compile && npm run hardhat:mumbai deploy-bridgerton --verify"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "chai": "^4.3.6",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.4",
    "hardhat": "^2.9.3",
    "hardhat-deploy": "^0.11.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@openzeppelin/contracts": "^4.5.0",
    "@poanet/solidity-flattener": "^3.0.7",
    "dotenv": "^16.0.0"
  }
}
